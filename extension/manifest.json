{
  "manifest_version": 2,
  "name": "compat-icon",
  "version": "1.0",
  "description": "",
  "permissions": [
    "<all_urls>",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "compat-data.js",
      "css-lexer.js",
      "css-tokenizer.js",
      "background.js"
    ]
  },
  "experiment_apis": {
    "styleEditor": {
      "schema": "experiments/style-editor/schema.json",
      "parent": {
        "scopes": ["addon_parent"],
        "script": "experiments/style-editor/api.js",
        "paths": [["experiments", "styleEditor"]]
      }
    }
  },
  "options_ui": {
    "page": "options.html"
  },
  "page_action": {
    "default_icon": "images/icon.svg",
    "default_popup": "popup.html",
    "default_title": "Compatiblity Icon",
    "show_matches": ["file://*/*", "http://*/*", "https://*/*"]
  },
  "web_accessible_resources": [
    "images/error.svg",
    "images/ok.svg",
    "images/warning.svg"
  ]
}
