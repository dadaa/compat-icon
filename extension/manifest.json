{
  "manifest_version": 2,
  "name": "compat-icon",
  "version": "1.0",
  "description": "",
  "permissions": [
    "<all_urls>",
    "tabs"
  ],
  "background": {
    "scripts": [
      "compat-data.js",
      "css-lexer.js",
      "css-tokenizer.js",
      "current-browser.js",
      "background.js"
    ]
  },
  "page_action": {
    "default_icon": "images/icon.svg",
    "default_title": "Compatiblity Icon",
    "show_matches": ["file://*/*", "http://*/*", "https://*/*"]
  },
  "web_accessible_resources": [
    "images/error.svg",
    "images/ok.svg",
    "images/warning.svg"
  ]
}
